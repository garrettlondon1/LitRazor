{
    "compilerOptions": {
      "target": "es2017",
      "module": "Node16",
      "moduleResolution": "node16",
      "lib": ["esnext.array", "esnext", "es2017", "dom"],
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true,
      "inlineSources": true,
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "outDir": "wwwroot/dist",
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "baseUrl": ".",
      "paths": {
        "lit": ["./node_modules/@lit"],
        "lit/*": ["./node_modules/@lit/*"],
        "api/types": ["./lib/api/types.gen.js"],
        "api/sdk": ["./lib/api/sdk.gen.js"]
      }
    },
    "include": ["Pages/**/*.ts", "lib/**/*.ts"]
  }